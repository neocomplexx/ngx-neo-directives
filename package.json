{
  "name": "neo-directives",
  "version": "8.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-license": "copy .\\LICENSE .\\dist\\ngx-neo-directives",
    "copy-license_mat": "copy .\\LICENSE .\\dist\\ngx-neo-directives-mat",
    "copy-readme": "copy .\\README.md .\\dist\\ngx-neo-directives",
    "copy-readme_mat": "copy .\\README.md .\\dist\\ngx-neo-directives-mat",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "copy-files_mat": "npm run copy-license_mat && npm run copy-readme_mat",
    "build_lib": "ng build ngx-neo-directives",
    "build_lib_mat": "ng build ngx-neo-directives-mat",
    "npm_pack": "cd dist/ngx-neo-directives && npm pack",
    "npm_pack_mat": "cd dist/ngx-neo-directives-mat && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "package_mat": "npm run build_lib_mat && npm run copy-files_mat && npm run npm_pack_mat",
    "debug": "ng build ngx-neo-directives && ng serve --vendorSourceMap",
    "debug_mat": "ng build ngx-neo-directives-mat && ng serve --vendorSourceMap"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ng-bootstrap/ng-bootstrap": "^5.3",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
