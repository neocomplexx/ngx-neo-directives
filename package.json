{
  "name": "neo-directives",
  "version": "9.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy-license": "cp ./LICENSE ./dist\\ngx-neo-directives",
    "copy-license_mat": "cp ./LICENSE ./dist\\ngx-neo-directives-mat",
    "copy-readme": "cp ./README.md ./dist\\ngx-neo-directives",
    "copy-readme_mat": "cp ./README.md ./dist\\ngx-neo-directives-mat",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "copy-files_mat": "npm run copy-license_mat && npm run copy-readme_mat",
    "build_lib": "ng build ngx-neo-directives --prod",
    "build_lib_mat": "ng build ngx-neo-directives-mat --prod",
    "npm_pack": "cd dist/ngx-neo-directives && npm pack",
    "npm_pack_mat": "cd dist/ngx-neo-directives-mat && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "package_mat": "npm run build_lib_mat && npm run copy-files_mat && npm run npm_pack_mat",
    "debug": "npm run build_lib && ng serve --vendorSourceMap",
    "debug_mat": "npm run build_lib_mat && ng serve --vendorSourceMap"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.4.1",
    "rxjs": "^6.5.5",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslib": "^1.13.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
